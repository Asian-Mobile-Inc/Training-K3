<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/blankFragment">
    <fragment
        android:id="@+id/downloadedFragment"
        android:name="com.example.roomdb.view.fragment.DownloadedFragment"
        android:label="DownloadedFragment" >
        <action
            android:id="@+id/action_downloadedFragment_to_apiFragment"
            app:destination="@id/apiFragment" />
    </fragment>
    <fragment
        android:id="@+id/favoriteFragment2"
        android:name="com.example.roomdb.view.fragment.FavoriteFragment"
        android:label="FavoriteFragment" >
        <action
            android:id="@+id/action_favoriteFragment2_to_apiFragment"
            app:destination="@id/apiFragment" />
    </fragment>
    <fragment
        android:id="@+id/apiFragment"
        android:name="com.example.roomdb.view.fragment.ApiFragment"
        android:label="ApiFragment" >
        <action
            android:id="@+id/action_apiFragment_to_downloadedFragment"
            app:destination="@id/downloadedFragment" />
        <action
            android:id="@+id/action_apiFragment_to_favoriteFragment2"
            app:destination="@id/favoriteFragment2" />
    </fragment>
    <fragment
        android:id="@+id/blankFragment"
        android:name="com.example.roomdb.BlankFragment"
        android:label="fragment_blank"
        tools:layout="@layout/fragment_blank" />
</navigation>